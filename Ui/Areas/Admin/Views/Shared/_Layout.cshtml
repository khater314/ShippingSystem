                                <div class="col-3"><i class="flag-icon flag-icon-eg" title="eg" id="eg"></i></div>
                                <div class="preview-item-content d-flex align-items-start flex-column justify-content-center">
                                    <h6 class="preview-subject font-weight-normal mb-1">@ResShared.Lang_Arabic</h6>
                                </div>
                            </a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item preview-item" 
                            href="/ChangeLanguage?culture=en&returnUrl=@Context.Request.Path">
                                <div class="col-3"><i class="flag-icon flag-icon-us" title="us" id="us"></i></div>
                                <div class="preview-item-content d-flex align-items-start flex-column justify-content-center">
                                    <h6 class="preview-subject font-weight-normal mb-1">@ResShared.Lang_English</h6>
                                </div>
                            </a>
                        </div>
