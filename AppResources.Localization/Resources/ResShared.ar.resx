	<!--                                                                          -->
	<!-- Btn_     : الأزرار - كل ما هو زر في الموقع                              -->
	<!--            مثال: Btn_Save = حفظ                                         -->
	<!--                                                                          -->
	<!-- Lbl_     : تسميات الحقول - الأسماء اللي تظهر جنب الخانات                -->
	<!--            مثال: Lbl_Name = الاسم                                       -->
