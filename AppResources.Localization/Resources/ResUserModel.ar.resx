	<!--                                                                          -->
	<!-- Page_  : عناوين الصفحات (H1، عنوان الصفحة)                              -->
	<!--         مثال: Page_Dashboard = لوحة التحكم                              -->
	<!--                                                                          -->
	<!-- Btn_   : الأزرار والعناصر القابلة للنقر                                 -->
	<!--         مثال: Btn_Save = حفظ، Btn_Delete = حذف                          -->
	<!--                                                                          -->
	<!-- Lbl_   : تسميات الحقول وعناوين عرض البيانات                            -->
	<!--         مثال: Lbl_Name = الاسم، Lbl_Email = البريد الإلكتروني          -->
	<!--                                                                          -->
	<!-- Menu_  : عناصر القوائم                                                  -->
	<!--         مثال: Menu_Home = الرئيسية، Menu_Products = المنتجات           -->
	<!--                                                                          -->
	<!-- Msg_   : رسائل النظام (نجاح، خطأ، تحذير، معلومات)                      -->
	<!--         مثال: Msg_Success = تمت العملية بنجاح                           -->
	<!--                                                                          -->
	<!-- Sec_   : عناوين الأقسام داخل الصفحة                                     -->
	<!--         مثال: Sec_PersonalInfo = المعلومات الشخصية                      -->
	<!--                                                                          -->
	<!-- Opt_   : الخيارات (نعم/لا، مفعل/غير مفعل، ذكر/أنثى)                    -->
	<!--         مثال: Opt_Yes = نعم، Opt_Active = مفعل                          -->
	<!--                                                                          -->
	<!-- Fld_   : النصوص الإرشادية داخل الحقول (Placeholders)                   -->
	<!--         مثال: Fld_Search = ابحث...                                      -->
	<!--                                                                          -->
	<!-- Tbl_   : رؤوس أعمدة الجداول                                             -->
	<!--         مثال: Tbl_ID = المعرف، Tbl_Name = الاسم                         -->
	<!--                                                                          -->
	<!-- ======================================================================== -->

