                        <div class="tiles warning"></div>
                        <div class="tiles danger"></div>
                        <div class="tiles info"></div>
                        <div class="tiles dark"></div>
                        <div class="tiles default"></div>
                    </div>
                </div>
                <div id="layout-settings" class="settings-panel">
                    <i class="settings-close mdi mdi-close"></i>
                    <div class="d-flex align-items-center justify-content-between border-bottom">
                        <p class="settings-heading font-weight-bold border-top-0 mb-3 ps-3 pt-0 border-bottom-0 pb-0">@ResShared.Lbl_TemplateDemos</p>
                    </div>
                    <div class="demo-screen-wrapper">
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/vertical-default-light/index.html" class="demo-thumb-image" id="theme-light-switch">
                            <img src="~/admin/images/demo/vertical-default.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/vertical-default-dark/index.html" class="demo-thumb-image">
                            <img src="~/admin/images/demo/vertical-dark.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/horizontal-default-light/index.html" class="demo-thumb-image" id="theme-dark-switch">
                            <img src="~/admin/images/demo/horizontal-menu-light.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/horizontal-default-dark/index.html" class="demo-thumb-image">
                            <img src="~/admin/images/demo/horizontal-menu-dark.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/vertical-dark-sidebar/index.html" class="demo-thumb-image">
                            <img src="~/admin/images/demo/dark-sidebar.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/vertical-boxed/index.html" class="demo-thumb-image">
                            <img src="~/admin/images/demo/boxed-layout.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/vertical-icon-menu/index.html" class="demo-thumb-image">
                            <img src="~/admin/images/demo/icon-menu.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/vertical-compact/index.html" class="demo-thumb-image">
                            <img src="~/admin/images/demo/compact-menu.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/vertical-fixed/index.html" class="demo-thumb-image">
                            <img src="~/admin/images/demo/fixed-menu.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/vertical-hidden-toggle/index.html" class="demo-thumb-image">
                            <img src="~/admin/images/demo/toggle-menu.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                        <a href="https://www.bootstrapdash.com/demo/majestic-admin-pro/template/demo/vertical-toggle-overlay/index.html" class="demo-thumb-image">
                            <img src="~/admin/images/demo/toggle-overlay-menu.png" alt="@ResShared.Plh_DemoImage">
                        </a>
                    </div>
                </div>
            </div>
            <div id="right-sidebar" class="settings-panel">
                <i class="settings-close mdi mdi-close"></i>
                <ul class="nav nav-tabs" id="setting-panel" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" id="todo-tab" data-bs-toggle="tab" href="#todo-section" role="tab" aria-controls="todo-section" aria-expanded="true">@ResShared.Lbl_TodoTab</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="chats-tab" data-bs-toggle="tab" href="#chats-section" role="tab" aria-controls="chats-section">@ResShared.Lbl_ChatsTab</a>
                    </li>
                </ul>
                <div class="tab-content" id="setting-content">
                    <div class="tab-pane fade show active scroll-wrapper" id="todo-section" role="tabpanel" aria-labelledby="todo-section">
                        <div class="add-items d-flex px-3 mb-0">
                            <form class="form w-100">
                                <div class="form-group d-flex">
                                    <input type="text" class="form-control todo-list-input" placeholder="@ResShared.Plh_AddTodo">
                                    <button type="submit" class="add btn btn-primary todo-list-add-btn" id="add-task">@ResShared.Btn_Add</button>
                                </div>
                            </form>
                        </div>
                        <div class="list-wrapper px-3">
                            <ul class="d-flex flex-column-reverse todo-list">
                                <li>
                                    <div class="form-check">
                                        <label class="form-check-label">
                                            <input class="checkbox" type="checkbox"> @ResShared.Todo_TeamReview
                                        </label>
                                    </div>
                                    <i class="remove mdi mdi-close-circle-outline"></i>
                                </li>
                                <li>
                                    <div class="form-check">
                                        <label class="form-check-label">
                                            <input class="checkbox" type="checkbox"> @ResShared.Todo_PreparePresentation
                                        </label>
                                    </div>
                                    <i class="remove mdi mdi-close-circle-outline"></i>
                                </li>
                                <li>
                                    <div class="form-check">
                                        <label class="form-check-label">
                                            <input class="checkbox" type="checkbox"> @ResShared.Todo_ResolveLowPriorityTickets
                                        </label>
                                    </div>
                                    <i class="remove mdi mdi-close-circle-outline"></i>
                                </li>
                                <li class="completed">
                                    <div class="form-check">
                                        <label class="form-check-label">
                                            <input class="checkbox" type="checkbox" checked> @ResShared.Todo_ScheduleMeeting
                                        </label>
                                    </div>
                                    <i class="remove mdi mdi-close-circle-outline"></i>
                                </li>
                                <li class="completed">
                                    <div class="form-check">
                                        <label class="form-check-label">
                                            <input class="checkbox" type="checkbox" checked> @ResShared.Todo_ProjectReview
                                        </label>
                                    </div>
                                    <i class="remove mdi mdi-close-circle-outline"></i>
                                </li>
                            </ul>
                        </div>
                        <div class="events py-4 border-bottom px-3">
